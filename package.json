  "scripts": {
    "clean:build": "rm -rf img/ && rm bundle.css && rm bundle.js",
    "dev": "rollup -c ./internals/rollup.config.dev.js -w",
    "build": "yarn minify-images && rollup -c ./internals/rollup.config.prod.js  --environment PRODUCTION",
    "build:serve": "yarn minify-images && rollup -c ./internals/rollup.config.prod.js --environment SERVE_PRODUCTION",
    "minify-images": "node internals/compressImages.js",
    "lint": "eslint --ext .js src/ --fix"
